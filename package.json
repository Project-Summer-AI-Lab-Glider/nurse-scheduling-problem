{
  "name": "nurse-scheduling-problem",
  "version": "2.0.0",
  "private": true,
  "description": "Application for managining personal in medical foundations",
  "author": "KN Glider",
  "build": {
    "appId": "<com.your_app>"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.9.5",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/pickers": "3.2.10",
    "@popperjs/core": "^2.6.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@sentry/integrations": "^6.2.0",
    "@sentry/react": "^6.2.0",
    "@sentry/tracing": "^6.2.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "cypress-plugin-tab": "^1.0.5",
    "cross-env": "^7.0.2",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "electron": "^10.1.5",
    "electron-is-dev": "^1.2.0",
    "emailjs-com": "^2.6.4",
    "exceljs": "^4.1.1",
    "file-saver": "^2.0.2",
    "file-type": "^16.2.0",
    "history": "4.10.1",
    "html2canvas": "^1.0.0-rc.7",
    "i18next": "^20.2.1",
    "immutable": "^4.0.0-rc.12",
    "istanbul-lib-coverage": "^3.0.0",
    "jspdf": "^2.3.1",
    "jszip": "^3.6.0",
    "lodash": "^4.17.20",
    "npm": "^7.5.3",
    "nyc": "^15.1.0",
    "pouchdb-browser": "^7.2.2",
    "react": "^17.0.2",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.13.1",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.8.13",
    "react-icons": "^3.11.0",
    "react-merge-refs": "^1.1.0",
    "react-popper": "^2.2.4",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.1.0",
    "redux-undo": "^1.0.1",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "wait-on": "^5.2.1"
  },
  "scripts": {
    "b-start": "react-scripts -r @cypress/instrument-cra start",
    "b-build": "react-scripts build",
    "eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm build && electron-builder --publish=always",
    "build": "npm build && npm electron-build",
    "start": "concurrently \"cross-env BROWSER=none npm run b-start\" \"node scripts/await.js && electron .\"",
    "lint": "eslint .",
    "lint-fix": "eslint . -c .eslintrc.pre-push.json --fix",
    "lint-warn": "eslint . -c .eslintrc.pre-push.json --max-warnings=0",
    "cypress:open": "cypress open",
    "test": "cypress run",
    "coverage": "nyc report --reporter=text-summary"
  },
  "nyc": {
    "reporter": [
      "text",
      "json",
      "html"
    ],
    "include": "src",
    "exclude": [
      "src/assets/**"
    ],
    "all": true
  },
  "devDependencies": {
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/snapshot": "^2.1.7",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/classnames": "^2.3.1",
    "@types/history": "^4.7.8",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.21",
    "@types/pouchdb": "^6.4.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chokidar": "^3.5.2",
    "concurrently": "^5.3.0",
    "cypress": "^6.2.0",
    "cypress-file-upload": "^4.1.1",
    "cypress-log-to-output": "^1.1.2",
    "electron-builder": "^22.9.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "semantic-release": "^17.4.0",
    "typedoc": "^0.19.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint-fix && npm run lint-warn"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "cypress/**/*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
